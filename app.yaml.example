runtime: python38
entrypoint: gunicorn -c gunicorn.conf.py docsearch.api.search:api
service: docsearch

runtime_config:
  python_version: 3

env_variables:
  REDIS_HOST: "YOUR HOST"
  REDIS_PASSWORD: "YOUR PASSWORD"

# Low-cost test settings -- delete when live.
manual_scaling:
  instances: 1
resources:
  cpu: 1
  memory_gb: 0.5
  disk_size_gb: 10